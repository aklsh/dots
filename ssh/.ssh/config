Host *
    HostKeyAlgorithms ssh-rsa,ssh-ed25519
    TCPKeepAlive yes
	ControlMaster auto
	ControlPath ~/.ssh/control/%r@%h:%p
	ControlPersist 1800
	Compression yes
Host github.com
    User git
	ControlMaster auto
	ControlPersist 120
    IdentityFile ~/Secrets/SSH/git
Host gitlab.com
    User git
    IdentityFile ~/Secrets/SSH/git
Host git.sr.ht
    User git
    IdentityFile ~/Secrets/SSH/git
Host athena
    User aklsh
    HostName fedi.aklsh.me
    IdentityFile ~/Secrets/SSH/athena
    ServerAliveInterval 120
    ServerAliveCountMax 5
