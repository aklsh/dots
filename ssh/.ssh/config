    User git
    IdentityFile ~/Secrets/SSH/git
Host gitlab.com
    User git
    IdentityFile ~/Secrets/SSH/git
Host git.sr.ht
    User git
    IdentityFile ~/Secrets/SSH/git
Host athena
    User aklsh
    HostName fedi.aklsh.me
    IdentityFile ~/Secrets/SSH/athena
    ServerAliveInterval 120
    ServerAliveCountMax 5
